{
	"env": {
			"browser": true,
			"es2021": true,
			"node": true,
			"jest/globals": true
	},
	"extends": [
			"plugin:react/recommended",
			"plugin:jest/recommended",
			"airbnb"
	],
	"parserOptions": {
			"ecmaFeatures": {
					"jsx": true
			},
			"ecmaVersion": 2020,
			"sourceType": "module"
	},
	"plugins": [
			"react",
			"jest"
	],
	"rules": {
	}
}
